[*.cs]

# CA1031: Do not catch general exception types
dotnet_diagnostic.CA1031.severity = none

# IDE0069: 应释放可释放的字段
dotnet_diagnostic.IDE0069.severity = none

# CA1815: Override equals and operator equals on value types
dotnet_diagnostic.CA1815.severity = none

# IDE0029: 使用 COALESCE 表达式
dotnet_style_coalesce_expression = true:none

# IDE0067: 丢失范围之前释放对象
dotnet_diagnostic.IDE0067.severity = none

# IDE0060: 删除未使用的参数
dotnet_code_quality_unused_parameters = non_public:none

# CA1819: Properties should not return arrays
dotnet_diagnostic.CA1819.severity = none

# IDE0031: 使用 null 传播
dotnet_style_null_propagation = true:none

# IDE0018: 内联变量声明
csharp_style_inlined_variable_declaration = true:none

# IDE0052: 删除未读的私有成员
dotnet_diagnostic.IDE0052.severity = none
